<div id="add-story">
  <div class="container">
    <h1>Add a story to your journal</h1>
    <div class="q-and-a">
      <hr class="mt-4 mb-4"/>
        <div class="form-row">
          <label for="Title">The title</label>
          <input class="form-control form-control-sm" type="text" id="title" />
          <label for="text">Your story</label>
          <textarea class="form-control" name="text" id="story"></textarea>
          <button type="button" class="btn btn-secondary btn-sm" id="submit">post</button>
        </div>
    </div>
  </div>
</div>
<%- /* Expose locals as `window.SAILS_LOCALS` :: */ exposeLocalsToBrowser() %>
